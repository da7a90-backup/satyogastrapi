{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "event"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "subtitle": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "startDate": {
      "type": "datetime"
    },
    "endDate": {
      "type": "datetime"
    },
    "duration": {
      "type": "component",
      "repeatable": false,
      "component": "sections.duration"
    },
    "location": {
      "type": "enumeration",
      "enum": [
        "onsite",
        "online"
      ]
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "teacher": {
      "type": "string"
    },
    "isSoldOut": {
      "type": "boolean"
    },
    "price": {
      "type": "integer"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "sections.seo"
    },
    "hiddenTag": {
      "type": "string"
    }
  }
}
